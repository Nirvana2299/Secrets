<%- include('partials/header') %>

  <div class="jumbotron j text-center">
    <div class="container">
      <i class="fas fa-key fa-6x"></i>
      <h1 class="display-3">You've Discovered My Secret!</h1>

      <% secretSubmitted.forEach((user) => { %>
         <p class="secret-text">
        <%= user.secret %>
          </p>
          <% }) %>
          <hr>

          <a class="btn btn-light btn-lg" href="/logout" role="button">Log Out</a>
          <a type="submit" class="btn btn-dark btn-lg" name="submit" href="/submit" role="button">Submit a Secret</a>
    </div>
  </div>

  <%- include('partials/footer') %>